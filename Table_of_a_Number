#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,a;
    printf("Enter the number whose table you want: ");
    scanf("%d",&a);
	  printf("Enter the number till which you need table of a: ");
	  scanf("%d",&n);

	  int i,j;
	  i=1;
	  while(i<=n)
	  {
		  j=i*a;
		  printf("%d * %d = %d\n",a,i,j);
		  i++;
	  };

	  return 0;
}
