#include <stdio.h>
#include <string.h>
struct detail
{
    char name[20];
    int marks;
    char course[20];
};
int main() {
    struct detail d;
    strcpy(d.name,"Harman");
    d.marks = 90;
    strcpy(d.course,"CSE1");
    
    printf("Details: %s %d %s",d.name,d.marks,d.course);
    
    return 0;
}
