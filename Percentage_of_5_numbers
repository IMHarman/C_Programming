#include <stdio.h>
#include <stdlib.h>

int main()
{
    int no1, no2, no3, no4, no5, per;
    printf("\n Enter marks of 5 subjects: ");
    printf("\n Subject 1: ");
    scanf("%d", &no1);
    printf("\n Subject 2: ");
    scanf("%d", &no2);
    printf("\n Subject 3: ");
    scanf("%d", &no3);
    printf("\n Subject 4: ");
    scanf("%d", &no4);
    printf("\n Subject 5: ");
    scanf("%d", &no5);
    per = (no1+no2+no3+no4+no5)/5;
    printf("\n Percentage: ");
    printf("%d", per);
}
